{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false,
        "minify": true,
        "pure": true,
        "transpileTemplateLiterals": true
      }
    ],
    "babel-plugin-polished",
    "@babel/plugin-transform-react-jsx",
    "babel-plugin-transform-merge-sibling-variables",
    "@babel/plugin-transform-reserved-words"
  ]
}
